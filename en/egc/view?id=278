			function checkForm(form) {
				var errors = "";

			

				if (form.content.value == "") {
					errors += "Please select a file for upload.\n";
				}

		        
				if (errors == "") {
					return true;
				}
				else {
					alert(errors)
					return false;
				}
			}

		</script>
